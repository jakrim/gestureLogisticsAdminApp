{
  "expo": {
    "name": "Gesture Admin",
    "description": "A Gesture app supporting admins by tracking orders and runners.",
    "slug": "gestureAdmin",
    "privacy": "public",
    "platforms": ["ios", "android", "web"],
    "version": "0.0.1",
    "orientation": "portrait",
    "icon": "./assets/Gesture-icon-280.png",
    "splash": {
      "image": "./assets/Gesture-splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "bundleIdentifier": "com.gesture.logisticsadmin",
      "buildNumber": "1.0.0",
      "supportsTablet": true,
      "infoPlist": {}
    },
    "android": {
      "package": "com.gesture.logisticsadmin",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/Gesture-icon-280.png",
        "backgroundColor": "#ffffff"
      }
    }
  }
}
